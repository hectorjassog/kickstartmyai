{
    "project_name": "My AI Project",
    "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
    "project_description": "A FastAPI project with AI capabilities",
    "author_name": "Your Name",
    "author_email": "your.email@example.com",
    "version": "0.1.0",
    "python_version": "3.11",
    
    "database_type": ["postgresql", "mysql"],
    "database_name": "{{ cookiecutter.project_slug }}_db",
    "database_user": "{{ cookiecutter.project_slug }}_user",
    
    "include_openai": ["y", "n"],
    "include_anthropic": ["y", "n"],
    "include_gemini": ["n", "y"],
    
    "aws_region": ["us-east-1", "us-west-2", "eu-west-1", "ap-southeast-1"],
    "ecs_cluster_name": "{{ cookiecutter.project_slug }}-cluster",
    
    "include_redis": ["y", "n"],
    "include_monitoring": ["y", "n"],
    "include_load_testing": ["y", "n"],
    
    "environment": ["development", "staging", "production"],
    "debug": "true",
    "log_level": ["INFO", "DEBUG", "WARNING", "ERROR"],
    
    "use_https": ["y", "n"],
    "domain_name": "example.com",
    
    "_templates_suffix": "",
    "_output_dir": "{{ cookiecutter.project_slug }}"
}
